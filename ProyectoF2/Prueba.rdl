  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        TOP (5) YEAR(FECHA.fecha) AS year, MONTH(FECHA.fecha) AS month, SUM(COMPRA.precio_unitario) AS Total
FROM            COMPRA INNER JOIN
                         FECHA ON COMPRA.id_fecha = FECHA.id_fecha
GROUP BY YEAR(FECHA.fecha), MONTH(FECHA.fecha)
ORDER BY Total DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="month">
          <DataField>month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
